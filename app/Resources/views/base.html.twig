<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}mPurpose - Multipurpose Feature Rich Bootstrap Template{% endblock %}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/vendors.css' | asset_version ) }}"/>

    <!--[if lte IE 8]>
    <link rel="stylesheet" href="{{ asset('css/leaflet.ie.css' | asset_version ) }}"/>
    <![endif]-->

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,600,800' rel='stylesheet' type='text/css'>

    <link rel="stylesheet" href="{{ asset('css/main.css' | asset_version ) }}"/>
    {% endblock %}
    {% block javascripts_head %}
    <script src="{{ asset('js/modernizr.js' | asset_version ) }}"></script>
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <!-- Javascripts -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js' | asset_version ) }}"><\/script>')</script>
    <script src="{{ asset('js/vendors.js' | asset_version ) }}"></script>

    <script src="http://cdn.leafletjs.com/leaflet-0.5.1/leaflet.js"></script>

    <script src="{{ asset('js/site.js' | asset_version ) }}"></script>
{% endblock %}
</body>
</html>
